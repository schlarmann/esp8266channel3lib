{
    "name": "esp8266channel3lib",
    "version": "1.0.0",
    "keywords": "tvout, esp8266, tv, graphics",
    "description": "A library to emulate an analogue tv station on channel 3 with an ESP8266. Simply attach an antenna to the RX pin and tune a TV to channel 3. Uses I2S and DMA.",
    "repository":
    {
      "type": "git",
      "url": "https://github.com/schlarmann/esp8266channel3lib"
    },
    "authors":
    [
      {
          "name": "Paul Schlarmann",
          "url": "https://github.com/schlarmann"
      }
    ],
    "frameworks": "arduino",
    "platforms": [
      "espressif8266"
    ],
    "headers": [
        "esp8266channel3lib.h"
    ],	
    "examples": [
        {
            "name": "SimpleTextDemo(PAL)",
            "base": "examples/1_SimpleTextDemoPAL",
            "files": [
                "1_SimpleTextDemoPAL.ino"
            ]
        },

        {
          "name": "SimpleTextDemo(NTSC)",
          "base": "examples/1_SimpleTextDemoNTSC",
          "files": [
              "1_SimpleTextDemoNTSC.ino"
          ]
        },
        {
            "name": "OTA demo",
            "base": "examples/2_OTADemo",
            "files": [
                "2_OTADemo.ino"
            ]
        }
    ]
  }